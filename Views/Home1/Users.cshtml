
@model List<ecommerce.Models.signup>

@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
   
}


<h2>List of all the members</h2>


<table>
    <tr>
        <th>
           Name
        </th>
        <th>
            Email
        </th>
                <th>
            Password
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
           @item.Id
        </td>
        <td>
            @item.Email
        </td>
        <td>
           @item.Password
        </td>
       
    </tr>
}

</table>
